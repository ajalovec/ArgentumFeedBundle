{
    "name": "argentum/feed-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to generate RSS Feed with custom namespaces and elements",
    "keywords": ["feed", "rss"],
    "homepage": "http://argentum.ua",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "javer@argentum.ua",
            "homepage": "http://argentum.ua"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "twig/twig": ">=1.10"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": { "Argentum\\FeedBundle": "" }
    },
    "target-dir": "Argentum/FeedBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
